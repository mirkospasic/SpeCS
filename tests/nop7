superquery:
PREFIX : <http://www.example.org/>

SELECT * WHERE {
	?x :takesCourse ?c1 .
	?c1 :shortName "Cs200" .
	?x :takesCourse ?c2 .
	?c2 :shortName "Cs301" .
	?x :takesCourse _:b .
	_:b :shortName "Cs401" .
}


subquery:
PREFIX : <http://www.example.org/>

SELECT * WHERE {
	?x :takesCourse ?c1 .
	?c1 :shortName "Cs200" .
	?x :takesCourse ?c2 .
	?c2 :shortName "Cs301" .
}
